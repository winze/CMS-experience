<table class="table table-striped table-bordered" id="sortable" data-link="{{ path ('admin_menu_update_order_ajax') }}">
    <thead>
        <tr>
            <th width="15"></th>
            <th>Name</th>
            <th>Lien vers la page</th>
            <th>Actif ?</th>
            <th>Action</th>
        </tr>
    </thead>
    <tboby>
        {% if menus|length > 0 %}
            {% for menu in menus %}
                {% include 'WinzeBackendBundle:Backend:menu.list.line.html.twig' with {'menu':menu, 'key':1} %}
            {% endfor %}
        {% else %}
        <tr>
            <td colspan="5">Pas de menu créée</td>
        </tr>
        {% endif %}
    </tboby>
</table>